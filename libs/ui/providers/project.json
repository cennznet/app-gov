{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/providers/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ui/providers",
        "main": "libs/ui/providers/src/index.ts",
        "tsConfig": "libs/ui/providers/tsconfig.lib.json",
        "assets": ["libs/ui/providers/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui/providers/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/providers"],
      "options": {
        "jestConfig": "libs/ui/providers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
