{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/chain/cennznet/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/chain/cennznet",
        "main": "libs/chain/cennznet/src/index.ts",
        "tsConfig": "libs/chain/cennznet/tsconfig.lib.json",
        "assets": ["libs/chain/cennznet/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/chain/cennznet/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/chain/cennznet"],
      "options": {
        "jestConfig": "libs/chain/cennznet/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
