{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/node/artifacts/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/node/artifacts",
        "main": "libs/node/artifacts/src/index.ts",
        "tsConfig": "libs/node/artifacts/tsconfig.lib.json",
        "assets": ["libs/node/artifacts/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/node/artifacts/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
